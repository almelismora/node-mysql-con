<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <!-- <link rel='stylesheet' href='/stylesheets/style.css'> -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <style>
        section {
            display: flex;
            flex-direction: column;
            justify-content: center;
            height: 74vh;
            margin: 3rem 3rem;
        }
    </style>
  
  </head>
  <body>
    <nav class="navbar navbar-dark bg-dark">
      <a href="#" class="navbar-brand mx-auto p-4">API USERS</a>
    </nav>

    <section class="d-flex align-items-center">
        <h1 class="mb-4">Datos de los usuarios</h1>
        <!-- <%= apis %> -->
        <!-- <div class="d-flex flex-column align-items-start m-4">
            <% for (let i = 0; i < 10; i++) { %>
                <p><%= apis[i].name %></p>
            <% } %>
        </div> -->

        <table class="table table-striped table-bordered mt-4 w-75">
            <tr>
                <th>Name</th>
                <th>Username</th>
                <th>Email</th>
                <th>City</th>
            </tr>
            <% for(let i = 0; i < apis.length; i++){ %>
                <tr>
                    <td><%= apis[i].name%></td>
                    <td><%= apis[i].username%></td>
                    <td><%= apis[i].email%></td>
                    <td><%= apis[i].address.city%></td>
                </tr>
            <% } %>
        </table>
        
    </section>
    

    <footer class="bg-dark text-white d-flex justify-content-center pt-4 pb-2">
      <p>es footer</p>
    </footer>


  </body>
</html>
